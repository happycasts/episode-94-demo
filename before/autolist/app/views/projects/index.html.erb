<div class="container">
  <div class="nav">
    <span>Listing Projects</span>
    <%= link_to "Users", users_path %>
  </div>
  <ul class="wrapper">
    <% @projects.each do |project| %>
    <li>
      <% if project.name == "haoqicat" %>
        <%= image_tag "haoqicat.png" %>
      <% else %>
        <%= image_tag "happycasts.png" %>
      <% end %>
      <%= project.name %>
      <%= link_to 'Add collaborators', project, :class => "btn btn-success right" %>
    </li>
    <% end %>
  </ul>
</div>